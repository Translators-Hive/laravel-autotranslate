{
  "name": "translators-hive/laravel-autotranslate",
  "description": "Translators Hive Laravel Autotranslate Package",
  "type": "library",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Translators Hive Team",
      "email": "admin@translators-hive.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "illuminate/config": "^5.4|^6|^7|^8|^9|^9.0.0",
    "illuminate/console": "^5.4|^6|^7|^8|^9|^9.0.0",
    "illuminate/support": "^5.4|^6|^7|^8|^9|^9.0.0",
    "illuminate/filesystem": "^5.4|^6|^7|^8|^9|^9.0.0",
    "symfony/finder": "^3.2|^4|^5|^6"
  },
  "autoload": {
    "psr-4": {
      "TranslatorsHive\\LaravelAutoTranslate\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "TranslatorsHive\\LaravelAutoTranslate\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
